{
    "name": "eye4web/zf2-site-config",
    "description": "Site-configuration module for Zend Framework 2",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "configuration"
    ],
    "homepage": "https://github.com/Eye4web/ZF2-Site-Config",
    "authors": [
        {
            "name": "Daniel Str√∏m",
            "email": "ds@eye4web.dk"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    },
    "require": {
        "php": "^8.1",
        "laminas/laminas-config": "~2.3|~3.0",
        "laminas/laminas-servicemanager": "~2.3|~3.0",
        "ocramius/proxy-manager": "~1.0|~2.0"
    },
    "autoload": {
        "psr-0": {
            "Eye4web\\SiteConfig": "src/"
        }
    },
    "require-dev" : {
        "container-interop/container-interop": "^1.1",
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer" : "1.5.*",
        "laminas/laminas-captcha" : "~2.1",
        "codeclimate/php-test-reporter": "dev-master",
        "laminas/laminasframework": "~2.3",
        "ramsey/uuid": "2.7.3"
    }
}
